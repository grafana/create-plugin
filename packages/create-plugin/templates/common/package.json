{
  "name": "{{ kebabCase pluginName }}",
  "version": "1.0.0",
  "description": "{{ sentenceCase pluginDescription }}",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "{{ packageManagerName }} run lint --fix",
    "e2e": "{{ packageManagerName }} exec cypress install && {{ packageManagerName }} exec grafana-e2e run",
    "e2e:update": "{{ packageManagerName }} exec cypress install && {{ packageManagerName }} exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "{{ sentenceCase orgName }}",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "{{ grafanaVersion }}",
    "@grafana/e2e-selectors": "{{ grafanaVersion }}",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.51",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",{{#if_eq pluginType "app" || pluginType "scenesapp"}}
    "@types/react-router-dom": "^5.3.3",{{/if_eq}}
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "{{ grafanaVersion }}",
    "@grafana/runtime": "{{ grafanaVersion }}",
    "@grafana/ui": "{{ grafanaVersion }}",{{#if_eq pluginType "scenesapp"}}
    "@grafana/scenes": "^0.7.0",{{/if_eq}}
    "react": "17.0.2",
    "react-dom": "17.0.2",{{#if_eq pluginType "app" || pluginType "scenesapp"}}
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.0",{{/if_eq}}
    "tslib": "2.5.0"
  },
  "packageManager": "{{ packageManagerName }}@{{ packageManagerVersion }}"
}
