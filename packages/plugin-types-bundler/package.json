{
  "name": "@grafana/plugin-types-bundler",
  "version": "0.0.0",
  "description": "Bundle grafana plugin typescript types for sharing with other plugins",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin/run.js",
  "type": "module",
  "exports": {
    "node": {
      "import": "./index.js"
    },
    "default": "./index.js"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc --project tsconfig.json && chmod +x ./dist/bin/run.js",
    "dev": "tsc --project tsconfig.json --watch",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "directory": "packages/plugin-types-bundler",
    "url": "https://github.com/grafana/plugin-tools"
  },
  "keywords": [
    "grafana",
    "plugin",
    "typescript",
    "types",
    "bundle"
  ],
  "author": "Grafana labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/grafana/plugin-tools/issues"
  },
  "dependencies": {
    "@grafana/levitate": "^0.14.0",
    "jackw-dts-bundle-gen-test": "^9.5.1"
  },
  "devDependencies": {}
}
